{
  "description": "ASP.NET Core middleware for routing requests to application logic and for generating links.\r\nCommonly used types:\r\n Microsoft.AspNetCore.Routing.Route\r\nMicrosoft.AspNetCore.Routing.RouteCollection",
  "version": "1.0.3",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/routing"
    },
    "tags": [
      "aspnetcore",
      "routing"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Http.Extensions": "1.0.2",
    "Microsoft.AspNetCore.Routing.Abstractions": "1.0.3",
    "Microsoft.AspNetCore.Routing.DecisionTree.Sources": {
      "type": "build",
      "version": "1.0.3"
    },
    "Microsoft.Extensions.HashCodeCombiner.Sources": {
      "type": "build",
      "version": "1.0.1"
    },
    "Microsoft.Extensions.Logging.Abstractions": "1.0.2",
    "Microsoft.Extensions.ObjectPool": "1.0.1",
    "Microsoft.Extensions.Options": "1.0.2",
    "Microsoft.Extensions.PropertyHelper.Sources": {
      "type": "build",
      "version": "1.0.1"
    }
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {
      "dependencies": {
        "System.Collections": "4.0.11-*",
        "System.Text.RegularExpressions": "4.1.0-*"
      }
    }
  }
}